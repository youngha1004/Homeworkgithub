<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <!--<pattern>[%-5level] %-50msg - [%thread] %logger.%M\(\) %n</pattern>-->
            <pattern>[%-5level] [%thread] %logger.%M\(\) - %msg %n</pattern>
        </encoder>
    </appender>
    
	<root level="info">
        <appender-ref ref="STDOUT" />
    </root>
</configuration>